<!DOCTYPE html>
<html>
<head>
  <title>Final</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <div class="header">
      <div class="row">
        <div class="col-md-12">
          <h1>Business Plan Competition Database</h1>
        </div>
      </div>

      <div class="row">
        <div class="col-md-8">
          <%= link_to "Competitions", "/competitions", "class" => "item-link"  %> |
          <%= link_to "Startups", "/startups" %> 
        </div>
        <div class="pull-right">
           <%= link_to "Add a Competition", "/competitions/new", "class" => "item-link"  %> |
           <%= link_to "Add a Startup", "/startups/new" %>
          </div>
      </div>
      <hr>
      <h4 class="text-warning"><%= notice %></h4>
    </div>


    <%= yield %>

  <div>

</body>
</html>
