<% startup_id = params["id"] %>
<% startup = Startup.read(startup_id) %>

<h1><%= startup["name"]%>

<h3>About</h3>
<p><font size="3"><%= startup["description"] %></p>
	
<div>
<h3>Kellogg Founders</h3>
    <% founders = Kellogg_founder.where("startup_id" => startup["id"]) %>
<% founders.each do |founder| %>
<li>
<p><%= founder["name"]%>, <%= founder["program"]%> <%= founder["grad_year"]%></p>	
</li>
<% end %>
</div>


<p><%= link_to "Delete", "/startups/#{startup["id"]}", "method" => "delete" %></p>

